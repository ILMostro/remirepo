[remi-php70-test]
name=Remi's RPM repository - Testing - PHP 7.0 - Fedora $releasever - $basearch
#baseurl=http://rpms.remirepo.net/fedora/$releasever/test70/$basearch/
mirrorlist=http://rpms.remirepo.net/fedora/$releasever/test70/mirror
